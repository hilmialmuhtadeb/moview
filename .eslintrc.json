{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "linebreak-style": "off",
        "eol-last": "off",
        "import/no-extraneous-dependencies": "off",
        "no-trailing-spaces":"off",
        "arrow-body-style": "off"
    }
}
